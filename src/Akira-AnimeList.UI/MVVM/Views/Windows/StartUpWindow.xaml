<Window x:Class="Akira_AnimeList.UI.MVVM.Views.Windows.StartUpWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:Akira_AnimeList.UI.MVVM.Views.Windows"
        xmlns:viewModel="clr-namespace:Akira_AnimeList.UI.MVVM.ViewModels"
        d:DataContext="{d:DesignInstance Type=viewModel:StartUpWindowVM}"
        mc:Ignorable="d"
        Title="StartUpWindow"
        Height="800"
        Width="600"
        Background="{x:Null}"
        AllowsTransparency="True"
        WindowStyle="None"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <Frame Content="{Binding ScreenFrame}"
               NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
